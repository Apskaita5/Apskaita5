																			<xs:element name="LineNumber" type="SAFshorttextType">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Mokėjimo eilutės / įrašo numeris.</xs:documentation>
																					<xs:documentation xml:lang="en">Number of payment line / entry.</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="SourceDocumentID" type="SAFmiddle1textType" minOccurs="0">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Pirminio dokumento, kurio pagrindu atliktas / gautas mokėjimas, numeris.</xs:documentation>
																					<xs:documentation xml:lang="en">Number of the initial document on the basis of which payment was made / received.</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="AccountID" type="SAFmiddle2textType">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Subjekto apskaitos sistemoje naudojamos buhalterinės pinigų ir pinigų ekvivalentų / skolos sąskaitos kodas. Galima įtraukti subsąskaitų numerius. Gali apimti daug skirtingų lygių tam, kad būtų galima kuo geriau identifikuoti sąskaitą. Gali apimti išlaidų centrus, tokius kaip: įmonė, padalinys, regionas, grupė ir filialas / departamentas.</xs:documentation>
																					<xs:documentation xml:lang="en">A code of the financial account of cash and cash equivalents / debt used in the accounting system of the entity. Numbers of sub-accounts can be included. Can involve many different levels in order to identify the account as better as possible. Can cover cost centres such as: company, unit, region, group and branch / department.</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="Analyses" minOccurs="0">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Analizės informacija.</xs:documentation>
																					<xs:documentation xml:lang="en">Analysis information.</xs:documentation>
																				</xs:annotation>
																				<xs:complexType>
																					<xs:sequence>
																						<xs:element name="Analysis" type="AnalysisStructure" minOccurs="0" maxOccurs="unbounded">
																							<xs:annotation>
																								<xs:documentation xml:lang="lt">Didžiosios knygos analizės kodai.</xs:documentation>
																								<xs:documentation xml:lang="en">Analysis codes of the General ledger.</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																					</xs:sequence>
																				</xs:complexType>
																			</xs:element>
																			<xs:element name="CustomerID" type="SAFmiddle1textType">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Unikalus pirkėjo, debitoriaus numeris subjekto apskaitos sistemoje.</xs:documentation>
																					<xs:documentation xml:lang="en">Unique number of payer, debtor in the accounting system of the entity.</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="SupplierID" type="SAFmiddle1textType">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Unikalus tiekėjo, kreditoriaus numeris subjekto apskaitos sistemoje.</xs:documentation>
																					<xs:documentation xml:lang="en">Unique number of supplier, creditor in the accounting system of the entity.</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="Description" type="SAFlongtextType" minOccurs="0">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Apibūdinimas, apmokėjimo pagrindas (sąskaita faktūra, avanso apyskaita, darbo užmokesčio žiniaraštis, buhalterinė pažyma ir pan.).</xs:documentation>
																					<xs:documentation xml:lang="en">Description, basis of payment (invoice, Petty Cash Voucher, payroll sheet, accounting note, etc.).</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="DebitCreditIndicator">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Nuoroda, identifikuojanti eilutės sumos požymį (D - debetą / K - kreditą). Įrašas privalo atitikti įrašą, randamą didžiojoje knygoje. Pavyzdžiui, grąžinimas gali būti nurodytas neigiama suma (kontrarinėje sąskaitoje).</xs:documentation>
																					<xs:documentation xml:lang="en">Reference identifying an attribute of the line‘s amount (D – debit / K – credit). The entry must correspond to the entry found in the General ledger. For example, refund can be specified in a negative amount (in a contrary account).</xs:documentation>
																				</xs:annotation>
																				<xs:simpleType>
																					<xs:restriction base="SAFcodeType">
																						<xs:enumeration value="D"/>
																						<xs:enumeration value="K"/>
																					</xs:restriction>
																				</xs:simpleType>
																			</xs:element>
																			<xs:element name="PaymentLineAmount" type="AmountStructure">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Mokėjimo eilutės suma.</xs:documentation>
																					<xs:documentation xml:lang="en">Payment line amount.</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="Settlements" minOccurs="0">
														<xs:annotation>
															<xs:documentation xml:lang="lt">Informacija apie faktiškai gautas/suteiktas nuolaidas.</xs:documentation>
															<xs:documentation xml:lang="en">Information on discounts actually received/granted. </xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="Settlement" minOccurs="0" maxOccurs="unbounded">
																	<xs:annotation>
																		<xs:documentation xml:lang="lt">Informacija apie faktiškai gautą/suteiktą nuolaidą.</xs:documentation>
																		<xs:documentation xml:lang="en">Information on discount actually received/granted.</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="SettlementDiscount" type="SAFmiddle1textType" minOccurs="0">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Atsiskaitymo nuolaidos.</xs:documentation>
																					<xs:documentation xml:lang="en">Settlement discounts.</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="SettlementAmount" type="AmountStructure" minOccurs="0">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Atsiskaitymo suma.</xs:documentation>
																					<xs:documentation xml:lang="en">Settlement amount.</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="SettlementDate" type="SAFdate" minOccurs="0">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Atsiskaitymo data.</xs:documentation>
																					<xs:documentation xml:lang="en">Settlement date.</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="PaymentMechanism" minOccurs="0">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Mokėjimo būdas. Pasirinkimai: „GR“ (grynaisiais), „NG“ (negrynaisiais), „U“ (užskaita).</xs:documentation>
																					<xs:documentation xml:lang="en">Payment method. Options: “GR” (cash), “NG” (non-cash), “U” (netting).</xs:documentation>
																				</xs:annotation>
																				<xs:simpleType>
																					<xs:restriction base="SAFcodeType">
																						<xs:enumeration value="GR"/>
																						<xs:enumeration value="NG"/>
																						<xs:enumeration value="U"/>
																					</xs:restriction>
																				</xs:simpleType>
																			</xs:element>
																			<xs:element name="DebitCreditIndicator">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Debeto arba kredito požymis (D - debetas, K- kreditas). </xs:documentation>
																					<xs:documentation xml:lang="en">Debit or credit attribute (D – debit / K – credit).</xs:documentation>
																				</xs:annotation>
																				<xs:simpleType>
																					<xs:restriction base="SAFcodeType">
																						<xs:enumeration value="D"/>
																						<xs:enumeration value="K"/>
																					</xs:restriction>
																				</xs:simpleType>
																			</xs:element>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="GrossTotal" type="SAFmonetaryType" nillable="true">
														<xs:annotation>
															<xs:documentation xml:lang="lt">Bendra suma.</xs:documentation>
															<xs:documentation xml:lang="en">Total amount of payment document.</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="MovementOfGoods" minOccurs="0">
								<xs:annotation>
									<xs:documentation xml:lang="lt">Informacija apie prekių judėjimą, pvz., prekių užpajamavimas, nurašymas pardavus prekes ar sunaudojus gamyboje, pagamintos produkcijos užpajamavimas, nustatyta netektis, brokas ir kt.</xs:documentation>
									<xs:documentation xml:lang="en">Information on movement of goods, e.g. recording of goods, write-off after sale of goods or after use thereof in production, recording of finished products, loss determined, defective goods, etc.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="NumberOfMovementLines" type="xs:nonNegativeInteger" nillable="true">
											<xs:annotation>
												<xs:documentation xml:lang="lt">Prekių judėjimo eilučių skaičius per rinkmenos laikotarpį.</xs:documentation>
												<xs:documentation xml:lang="en">Number of lines of movement of goods during the file period.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="TotalQuantityReceived" type="SAFquantityType" nillable="true">
											<xs:annotation>
												<xs:documentation xml:lang="lt">Bendras gautų prekių skaičius.</xs:documentation>
												<xs:documentation xml:lang="en">Total number of goods received.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="TotalQuantityIssued" type="SAFquantityType" nillable="true">
											<xs:annotation>
												<xs:documentation xml:lang="lt">Bendras išduotų prekių skaičius.</xs:documentation>
												<xs:documentation xml:lang="en">Total number of goods issued.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="StockMovement" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation xml:lang="lt">Atsargų judėjimas.</xs:documentation>
												<xs:documentation xml:lang="en">Stock movement.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="MovementReference" type="SAFmiddle1textType">
														<xs:annotation>
															<xs:documentation xml:lang="lt">Unikali judėjimo (gabenimo) nuoroda (vardas, santrumpa, numeris), identifikuojanti atsargų judėjimo operaciją.</xs:documentation>
															<xs:documentation xml:lang="en">Unique movement (transportation) reference (forename, abbreviation, number) identifying a stock movement operation.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="MovementDate" type="SAFdate" nillable="true">
														<xs:annotation>
															<xs:documentation xml:lang="lt">Atsargų judėjimo dokumento data.</xs:documentation>
															<xs:documentation xml:lang="en">Date of stock movement document.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="MovementPostingDate" type="SAFdate" minOccurs="0">
														<xs:annotation>
															<xs:documentation xml:lang="lt">Judėjimo dokumento apskaitos data (gali skirtis nuo atsargų judėjimo dokumento datos).</xs:documentation>
															<xs:documentation xml:lang="en">Accounting date of movement document (may differ from the date of stock movement document).</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="TaxPointDate" type="SAFdate" minOccurs="0">
														<xs:annotation>
															<xs:documentation xml:lang="lt">Nurodyta apmokestinimo data, jei data nenurodyta, tuomet sąskaitos data.</xs:documentation>
															<xs:documentation xml:lang="en">Taxation date specified; invoice date if no date is specified.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="MovementType">
														<xs:annotation>
															<xs:documentation xml:lang="lt">Atsargų judėjimo tipas. Pasirinkimai: „PARD“ (Pardavimas), „PIR“ (Pirkimas), „PP“ (Pagaminta produkcija), „PG“ (Pardavimų grąžinimas), „PRG“ (Pirkimų grąžinimas), „VP“ (Vidinis perkėlimas), „N“ (Nurašymas), „KT“ (Kita).</xs:documentation>
															<xs:documentation xml:lang="en">Stock movement type. Options: “PARD” (Sale), “PIR” (Purchase), “PP” (Finished products), “PG“ (Return of sales), “PRG” (Return of purchases), “VP” (Internal transfer), “N“ (Write-off), „KT“ (Other).</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="SAFcodeType">
																<xs:enumeration value="PARD"/>
																<xs:enumeration value="PIR"/>
																<xs:enumeration value="PP"/>
																<xs:enumeration value="PG"/>
																<xs:enumeration value="PRG"/>
																<xs:enumeration value="VP"/>
																<xs:enumeration value="N"/>
																<xs:enumeration value="KT"/>
																<xs:enumeration value=""/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="SystemID" type="SAFmiddle1textType" minOccurs="0">
														<xs:annotation>
															<xs:documentation xml:lang="lt">Sistemos automatiškai sukurtas unikalus kodas / numeris konkrečiam dokumentui, susijusiam su ūkiniu įvykiu.</xs:documentation>
															<xs:documentation xml:lang="en">Unique code / number automatically created by the system for a specific document relating to economic event.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="DocumentReference" minOccurs="0">
														<xs:annotation>
															<xs:documentation xml:lang="lt">Atsargų judėjimą pagrindžiančio dokumento duomenys.</xs:documentation>
															<xs:documentation xml:lang="en">Data of the document justifying movement of stock.</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="DocumentType" type="SAFshorttextType">
																	<xs:annotation>
																		<xs:documentation xml:lang="lt">Dokumento tipas, pvz., PVM sąskaita faktūra, medžiagų nurašymo aktas ir kt.</xs:documentation>
																		<xs:documentation xml:lang="en">Data of the document justifying movement of stock.</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="DocumentNumber" type="SAFmiddle1textType">
																	<xs:annotation>
																		<xs:documentation xml:lang="lt">Dokumento numeris.</xs:documentation>
																		<xs:documentation xml:lang="en">Document number.</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="DocumentLine" type="SAFshorttextType" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation xml:lang="lt">Dokumento eilutės numeris.</xs:documentation>
																		<xs:documentation xml:lang="en">Document line number.</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="Lines">
														<xs:annotation>
															<xs:documentation xml:lang="lt">Dokumento eilutės / įrašai.</xs:documentation>
															<xs:documentation xml:lang="en">Document lines / entries.</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="Line" maxOccurs="unbounded">
																	<xs:annotation>
																		<xs:documentation xml:lang="lt">Dokumento eilutė/įrašas.</xs:documentation>
																		<xs:documentation xml:lang="en">Document line / entry.</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="LineNumber" type="SAFshorttextType">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Eilutės / įrašo numeris.</xs:documentation>
																					<xs:documentation xml:lang="en">Number of line / entry.</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="AccountID" type="SAFmiddle2textType">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Subjekto apskaitos sistemoje naudojamos buhalterinės atsargų sąskaitos kodas. Galima įtraukti subsąskaitų numerius. Gali apimti daug skirtingų lygių tam, kad būtų galima kuo geriau identifikuoti sąskaitą. Gali apimti išlaidų centrus, tokius kaip: įmonė, padalinys, regionas, grupė ir filialas / departamentas.</xs:documentation>
																					<xs:documentation xml:lang="en">A code of the financial account of stock used in the accounting system of the entity in which the records are made on the purchaser‘s, debtor‘s debts / receivables. Numbers of sub-accounts can be included. Can involve many different levels in order to identify the account as better as possible. Can cover cost centres such as: company, unit, region, group and branch / department.</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="TransactionID" type="SAFmiddle2textType">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Unikalus numeris, kuris priskirtas ūkinei operacijai / įvykiui ir kuris gali būti grindžiamas viena ar keliomis serijomis.</xs:documentation>
																					<xs:documentation xml:lang="en">A unique number assigned to economic transaction / event and which can be based on one or more series.</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="CustomerID" type="SAFmiddle1textType">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Unikalus pirkėjo, kliento numeris subjekto apskaitos sistemoje.</xs:documentation>
																					<xs:documentation xml:lang="en">Unique number of purchaser, customer in the accounting system of the entity.</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="SupplierID" type="SAFmiddle1textType">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Tiekėjo unikalus kodas subjekto apskaitos sistemoje.</xs:documentation>
																					<xs:documentation xml:lang="en">Unique number of the supplier in the accounting system of the entity.</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="ShipsTo" minOccurs="0">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Informacija apie pristatymą.</xs:documentation>
																					<xs:documentation xml:lang="en">Delivery information.</xs:documentation>
																				</xs:annotation>
																				<xs:complexType>
																					<xs:sequence>
																						<xs:element name="ShipTo" type="ShippingPointStructure" minOccurs="0" maxOccurs="unbounded">
																							<xs:annotation>
																								<xs:documentation xml:lang="lt">Pristatymo duomenys.</xs:documentation>
																								<xs:documentation xml:lang="en">Delivery data.</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																					</xs:sequence>
																				</xs:complexType>
																			</xs:element>
																			<xs:element name="ShipsFrom" minOccurs="0">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Išsiuntimo informacija.</xs:documentation>
																					<xs:documentation xml:lang="en">Dispatch information.</xs:documentation>
																				</xs:annotation>
																				<xs:complexType>
																					<xs:sequence>
																						<xs:element name="ShipFrom" type="ShippingPointStructure" minOccurs="0" maxOccurs="unbounded">
																							<xs:annotation>
																								<xs:documentation xml:lang="lt">Išsiuntimo duomenys.</xs:documentation>
																								<xs:documentation xml:lang="en">Dispatch data.</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																					</xs:sequence>
																				</xs:complexType>
																			</xs:element>
																			<xs:element name="ProductCode" type="SAFmiddle2textType">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Prekės / paslaugos kodas.</xs:documentation>
																					<xs:documentation xml:lang="en">Product / service code.</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="Quantity" type="SAFquantityType" nillable="true">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Tiekiamų prekių ar teikiamų paslaugų kiekis.</xs:documentation>
																					<xs:documentation xml:lang="en">Quantity of products supplied or services provided.</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:sequence minOccurs="0">
																				<xs:element name="UnitOfMeasure" type="SAFcodeType">
																					<xs:annotation>
																						<xs:documentation xml:lang="lt">Matavimo vieneto kiekis, pvz., 12 vnt. pakuotė.</xs:documentation>
																						<xs:documentation xml:lang="en">Quantity of the unit of measurement, e.g. a package of 12 pcs.</xs:documentation>
																					</xs:annotation>
																				</xs:element>
																				<xs:element name="UOMToUOMPhysicalStockConversionFactor" type="xs:decimal" nillable="true">
																					<xs:annotation>
																						<xs:documentation xml:lang="lt">Matavimo vienetų konversijos koeficientas, keičiant iš standartinio į atsargų.</xs:documentation>
																						<xs:documentation xml:lang="en">Conversion factor for units of measurement when changed from a standard one to stock.</xs:documentation>
																					</xs:annotation>
																				</xs:element>
																			</xs:sequence>
																			<xs:element name="BookValue" type="SAFmonetaryType" nillable="true">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Balansinė vertė.</xs:documentation>
																					<xs:documentation xml:lang="en">Carrying amount.</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="MovementSubType" type="SAFcodeType">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Detalesnis prekių judėjimo / būklės apibūdinimas, pvz., prekės gaminamos, baigta gamyba, netektis ir pan.</xs:documentation>
																					<xs:documentation xml:lang="en">A more detailed description of movement / status of goods, e.g. production of goods is in progress, production completed, loss, etc.</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="MovementComments" type="SAFlongtextType" minOccurs="0">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Prekių judėjimo aprašymas.</xs:documentation>
																					<xs:documentation xml:lang="en">Description of movement of goods.</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="TaxInformations" minOccurs="0">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Mokesčių informacija.</xs:documentation>
																					<xs:documentation xml:lang="en">Information of taxes.</xs:documentation>
																				</xs:annotation>
																				<xs:complexType>
																					<xs:sequence>
																						<xs:element name="TaxInformation" type="TaxInformationStructure" minOccurs="0" maxOccurs="unbounded">
																							<xs:annotation>
																								<xs:documentation xml:lang="lt">Informacija apie mokesčius.</xs:documentation>
																								<xs:documentation xml:lang="en">Information on taxes.</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																					</xs:sequence>
																				</xs:complexType>
																			</xs:element>
																			<xs:element name="MovementType" minOccurs="0">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Atsargų judėjimo tipas. Pasirinkimai: „PARD“ (Pardavimas), „PIR“ (Pirkimas), „PP“ (Pagaminta produkcija), „PG“ (Pardavimų grąžinimas), „PRG“ (Pirkimų grąžinimas), „VP“ (Vidinis perkėlimas), „N“ (Nurašymas), „KT“ (Kita).</xs:documentation>
																					<xs:documentation xml:lang="en">Stock movement type. Options: “PARD” (Sale), “PIR” (Purchase), “PP” (Finished products), “PG“ (Return of sales), “PRG” (Return of purchases), “VP” (Internal transfer), “N“ (Write-off), „KT“ (Other).</xs:documentation>
																				</xs:annotation>
																				<xs:simpleType>
																					<xs:restriction base="SAFcodeType">
																						<xs:enumeration value="PARD"/>
																						<xs:enumeration value="PIR"/>
																						<xs:enumeration value="PP"/>
																						<xs:enumeration value="PG"/>
																						<xs:enumeration value="PRG"/>
																						<xs:enumeration value="VP"/>
																						<xs:enumeration value="N"/>
																						<xs:enumeration value="KT"/>
																						<xs:enumeration value=""/>
																					</xs:restriction>
																				</xs:simpleType>
																			</xs:element>
																			<xs:element name="DebitCreditIndicator">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Debeto arba kredito požymis (D - debetas, K- kreditas).</xs:documentation>
																					<xs:documentation xml:lang="en">Debit or credit attribute (D – debit / K – credit).</xs:documentation>
																				</xs:annotation>
																				<xs:simpleType>
																					<xs:restriction base="SAFcodeType">
																						<xs:enumeration value="D"/>
																						<xs:enumeration value="K"/>
																					</xs:restriction>
																				</xs:simpleType>
																			</xs:element>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="GLTransactionID" type="SAFmiddle2textType" minOccurs="0">
														<xs:annotation>
															<xs:documentation xml:lang="lt">Elementas, skirtas pirminiam dokumentui su Didžiosios knygos elementu 3.4.4.1 „Ūkinės operacijos ar ūkinio įvykio Nr.“ susieti.</xs:documentation>
															<xs:documentation xml:lang="en">Element intended for linking of the initial document with the element of the General ledger 3.4.4.1 “Number of economic transaction or economic event“.</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="AssetTransactions" minOccurs="0">
								<xs:annotation>
									<xs:documentation xml:lang="lt">Ūkinės operacijos ar ūkiniai įvykiai dėl turto (materialaus, nematerialaus ir finansinio turto).</xs:documentation>
									<xs:documentation xml:lang="en">Economic transactions or economic asset (tangible, intangible and financial assets) events.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="NumberOfAssetTransactions" type="xs:nonNegativeInteger" nillable="true">
											<xs:annotation>
												<xs:documentation xml:lang="lt">Ūkinių operacijų ir ūkinių įvykių dėl turto (materialaus, nematerialaus ir finansinio turto) skaičius.</xs:documentation>
												<xs:documentation xml:lang="en">Number of economic transactions or economic asset (tangible, intangible and financial assets) events.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="AssetTransaction" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation xml:lang="lt">Ūkinė operacija ar ūkinis įvykis.</xs:documentation>
												<xs:documentation xml:lang="en">Economic transaction / economic event.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="AssetTransactionID" type="SAFmiddle2textType">
														<xs:annotation>
															<xs:documentation xml:lang="lt">Ūkinės operacijos ar ūkinio įvykio unikalus kodas.</xs:documentation>
															<xs:documentation xml:lang="en">Unique code of economic transaction / economic event.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="AssetID" type="SAFmiddle1textType">
														<xs:annotation>
															<xs:documentation xml:lang="lt">Unikalus turto identifikacinis kodas.</xs:documentation>
															<xs:documentation xml:lang="en">Unique assets identification code.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="AssetTransactionType">
														<xs:annotation>
															<xs:documentation xml:lang="lt">Ūkinės operacijos ar ūkinio įvykio tipas. Pasirinkimai: „I“ (Įsigijimas), „NUR“ (Nurašymas), „NUS“ (Nusidėvėjimas), „VJ“ (Vidinis judėjimas), „KT“ (Kita).</xs:documentation>
															<xs:documentation xml:lang="en">Type of economic transaction or economic event. Options: “I” (Acquisition), “NUR” (Write-off), “NUS” (Depreciation), “VJ” (Internal movement), “KT” (Other).</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="SAFcodeType">
																<xs:enumeration value="I"/>
																<xs:enumeration value="NUR"/>
																<xs:enumeration value="NUS"/>
																<xs:enumeration value="VJ"/>
																<xs:enumeration value="KT"/>
																<xs:enumeration value=""/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="Description" type="SAFlongtextType" minOccurs="0">
														<xs:annotation>
															<xs:documentation xml:lang="lt">Detalesnis ūkinės operacijos ar ūkinio įvykio aprašymas, pvz., ilgalaikio turto įsigijimas išperkamosios nuomos būdu ir kt.</xs:documentation>
															<xs:documentation xml:lang="en">A more detailed description of economic transaction or economic event, e.g. acquisition of non-current assets in the way of leasing, etc.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="AssetTransactionDate" type="SAFdate" nillable="true">
														<xs:annotation>
															<xs:documentation xml:lang="lt">Data, kada įvyko konkreti ūkinė operacija ar įvykis.</xs:documentation>
															<xs:documentation xml:lang="en">Date when a specific economic transaction or economic event took place.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="Supplier" minOccurs="0">
														<xs:annotation>
															<xs:documentation xml:lang="lt">Tiekėjas.</xs:documentation>
															<xs:documentation xml:lang="en">Supplier.</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="SupplierName" type="SAFmiddle2textType">
																	<xs:annotation>
																		<xs:documentation xml:lang="lt">Tiekėjo pavadinimas.</xs:documentation>
																		<xs:documentation xml:lang="en">Supplier name.</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="SupplierID" type="SAFmiddle1textType">
																	<xs:annotation>
																		<xs:documentation xml:lang="lt">Tiekėjo unikalus kodas subjekto apskaitos sistemoje.</xs:documentation>
																		<xs:documentation xml:lang="en">Unique number of the supplier in the accounting system of the entity.</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="PostalAddress" type="AddressStructure">
																	<xs:annotation>
																		<xs:documentation xml:lang="lt">Informacija apie turto tiekėjo adresą.</xs:documentation>
																		<xs:documentation xml:lang="en">Information on the assets supplier‘s address.</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="TransactionID" type="SAFmiddle2textType">
														<xs:annotation>
															<xs:documentation xml:lang="lt">Unikalus numeris, kuris priskirtas ūkinei operacijai / įvykiui ir kuris gali būti grindžiamas viena ar keliomis serijomis.</xs:documentation>
															<xs:documentation xml:lang="en">A unique number assigned to economic transaction / economic event and which can be based on one or more series.  </xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="AssetTransactionValuations">
														<xs:annotation>
															<xs:documentation xml:lang="lt">Ūkinės operacijos ar ūkinio įvykio vertės.</xs:documentation>
															<xs:documentation xml:lang="en">Values of economic transaction or economic event.</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="AssetTransactionValuation" maxOccurs="unbounded">
																	<xs:annotation>
																		<xs:documentation xml:lang="lt">Ūkinės operacijos ar ūkinio įvykio vertė.</xs:documentation>
																		<xs:documentation xml:lang="en">Value of economic transaction or economic event.</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="AssetValuationType" minOccurs="0">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Turto apskaitymo būdas po pirminio įvertinimo. Pasirinkimai:  „IS“ (Įsigijimo savikaina), „PV“ (Perkainojimo vertė), „KT“ (Kita).</xs:documentation>
																					<xs:documentation xml:lang="en">The method of accounting of the assets after the initial valuation. Options: “IS“ (Acquisition cost), “PV“ (Revaluation value), “KT” (Other).</xs:documentation>
																				</xs:annotation>
																				<xs:simpleType>
																					<xs:restriction base="SAFcodeType">
																						<xs:enumeration value="IS"/>
																						<xs:enumeration value="PV"/>
																						<xs:enumeration value="KT"/>
																					</xs:restriction>
																				</xs:simpleType>
																			</xs:element>
																			<xs:element name="AcquisitionAndProductionCostsOnTransaction" type="SAFmonetaryType" nillable="true">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Turto įsigijimo (pasigaminimo) savikaina ūkinės operacijos ar ūkinio įvykio datai.</xs:documentation>
																					<xs:documentation xml:lang="en">The cost of acquisition (production) of the assets on the day of economic transaction or economic event.</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="BookValueOnTransaction" type="SAFmonetaryType" nillable="true">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Turto balansinė vertė (suma) ūkinės operacijos ar ūkinio įvykio datai.</xs:documentation>
																					<xs:documentation xml:lang="en">Carrying amount of the assets on the day of economic transaction or economic event.</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="AssetTransactionAmount" type="SAFmonetaryType" nillable="true">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Ūkinės operacijos ar ūkinio įvykio suma (tikroji vertė).</xs:documentation>
																					<xs:documentation xml:lang="en">Amount of economic transaction / economic event (a real value).</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="DebitCreditIndicator">
																				<xs:annotation>
																					<xs:documentation xml:lang="lt">Debeto arba kredito požymis (D - debetas, K- kreditas).</xs:documentation>
																					<xs:documentation xml:lang="en">Debit or credit attribute (D – debit / K – credit).</xs:documentation>
																				</xs:annotation>
																				<xs:simpleType>
																					<xs:restriction base="SAFcodeType">
																						<xs:enumeration value="D"/>
																						<xs:enumeration value="K"/>
																					</xs:restriction>
																				</xs:simpleType>
																			</xs:element>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="InvoiceStructure">
		<xs:annotation>
			<xs:documentation xml:lang="lt">Sąskaitų faktūrų, PVM sąskaitų faktūrų struktūra.</xs:documentation>
			<xs:documentation xml:lang="en">Invoice structure, VAT invoice structure.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="InvoiceNo" type="SAFmiddle2textType">
				<xs:annotation>
					<xs:documentation xml:lang="lt">Sąskaitos faktūros serija ir numeris, PVM sąskaitos faktūros serija ir numeris.</xs:documentation>
					<xs:documentation xml:lang="en">Series and number of invoice, series and number of VAT invoice.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="CustomerInfo">
					<xs:annotation>
						<xs:documentation xml:lang="lt">Informacija apie pirkėją.</xs:documentation>
						<xs:documentation xml:lang="en">Information on purchaser (customer). Option available: 4.1.4.3 element.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:choice>
								<xs:element name="CustomerID" type="SAFmiddle1textType">
									<xs:annotation>
										<xs:documentation xml:lang="lt">Unikalus pirkėjo, kliento numeris subjekto apskaitos sistemoje.</xs:documentation>
										<xs:documentation xml:lang="en">Unique number of purchaser, customer in the accounting system of the entity.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Name" type="SAFmiddle2textType">
									<xs:annotation>
										<xs:documentation xml:lang="lt">Kliento pavadinimas.</xs:documentation>
										<xs:documentation xml:lang="en">Customer name.</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:choice>
							<xs:element name="BillingAddress" type="AddressStructure" minOccurs="0">
								<xs:annotation>
									<xs:documentation xml:lang="lt">Pirkėjo, kliento adresas. Elementų grupė užpildoma tik tada, kai informacija skiriasi nuo Pagrindinėje duomenų byloje pateikto pirkėjo adreso.</xs:documentation>
									<xs:documentation xml:lang="en">Address of purchaser, customer. A group of elements should be filled out only when information differs from the purchaser‘s address given in the Main data file.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="TaxRegistrationNumber" type="SAFmiddle1textType">
								<xs:annotation>
									<xs:documentation xml:lang="lt">Pirminiame dokumente nurodytas mokesčių mokėtojo PVM kodas (jo nesant, kitas dokumente nurodytas identifikatorius).</xs:documentation>
									<xs:documentation xml:lang="en">Taxpayer‘s VAT code specified in the initial document (in the absence thereof other identifier specified in the document).</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="TaxType">
								<xs:annotation>
									<xs:documentation xml:lang="lt">Pirminiame dokumente nurodytas mokesčių mokėtojo registracijos kodo tipas. Iš galimo reikšmių sąrašo šiame elemente galimi pasirinkimai: „PVM“ (PVM mokėtojo kodas), „MMR“ (Mokesčių mokėtojų registro identifikacinis kodas), „UUK“ (Ūkininko ūkio kodas), „KT“ (Kita).</xs:documentation>
									<xs:documentation xml:lang="en">Type of taxpayer‘s registration code specified in the initial document. Options available in this item from the possible list of values: “PVM” (VAT payer’s code), “MMR” (Identification code of taxpayers register), “UUK“(Farmer‘s farm code), „KT“ (Other).</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="SAFcodeType">
										<xs:enumeration value="PVM"/>
										<xs:enumeration value="MMR"/>
										<xs:enumeration value="UUK"/>
										<xs:enumeration value="KT"/>
										<xs:enumeration value=""/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="Country" type="ISOCountryCode">
								<xs:annotation>
									<xs:documentation xml:lang="lt">Šalies kodas iš dviejų raidžių pagal ISO 3166-1 alpha 2 arba ISO 3166-1 alpha 3 standartą, pavyzdžiui, NL arba NLD – Nyderlandai.</xs:documentation>
									<xs:documentation xml:lang="en">A 2-letter country code according to ISO 3166-1 alpha 2 or ISO 3166-1 alpha 3 standard, e.g. NL or NLD – the Netherlands.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="SupplierInfo">
					<xs:annotation>
						<xs:documentation xml:lang="lt">Informacija apie tiekėją. Galimas pasirinkimas 4.1.4.2 elementas. Pardavimo atvejais pasirenkamas, kai išrašoma debetinė sąskaita.</xs:documentation>
						<xs:documentation xml:lang="en">Information on supplier. Option available: element 4.1.4.2. Should be selected in cases of sale when a debit note is issued.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:choice>
								<xs:element name="SupplierID" type="SAFmiddle1textType">
									<xs:annotation>
										<xs:documentation xml:lang="lt">Tiekėjo unikalus kodas subjekto apskaitos sistemoje.</xs:documentation>
										<xs:documentation xml:lang="en">Unique number of the supplier in the accounting system of the entity.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Name" type="SAFmiddle2textType">
									<xs:annotation>
										<xs:documentation xml:lang="lt">Pavadinimas.</xs:documentation>
										<xs:documentation xml:lang="en">Name.</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:choice>
							<xs:element name="BillingAddress" type="AddressStructure" minOccurs="0">
								<xs:annotation>
									<xs:documentation xml:lang="lt">Tiekėjo adresas. Elementų grupė užpildoma tik tada, kai informacija skiriasi nuo Pagrindinėje duomenų byloje pateikto tiekėjo adreso.</xs:documentation>
									<xs:documentation xml:lang="en">Supplier‘s address. A group of elements should be filled out only when information differs from the supplier‘s address given in the Main data file.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="TaxRegistrationNumber" type="SAFmiddle1textType">
								<xs:annotation>
									<xs:documentation xml:lang="lt">Nurodomas mokesčių mokėtojo kodas (mokesčių mokėtojo registro (MMR) identifikacinis kodas ir / arba PVM mokėtojo kodas)ir / arba kitas identifikacinis kodas.</xs:documentation>
									<xs:documentation xml:lang="en">Taxpayer‘s VAT code specified in the initial document (in the absence thereof other identifier specified in the document).</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="TaxType">
								<xs:annotation>
									<xs:documentation xml:lang="lt">Pirminiame dokumente nurodytas mokesčių mokėtojo registracijos kodo tipas. Iš galimo reikšmių sąrašo šiame elemente galimi pasirinkimai: „PVM“ (PVM mokėtojo kodas), „MMR“ (Mokesčių mokėtojų registro identifikacinis kodas), „UUK“ (Ūkininko ūkio kodas), „KT“ (Kita).</xs:documentation>
									<xs:documentation xml:lang="en">Type of taxpayer‘s registration code specified in the initial document. Options available in this item from the possible list of values: “PVM” (VAT payer’s code), “MMR” (Identification code of taxpayers register), “UUK“(Farmer‘s farm code), „KT“ (Other).</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="SAFcodeType">
										<xs:enumeration value="PVM"/>
										<xs:enumeration value="MMR"/>
										<xs:enumeration value="UUK"/>
										<xs:enumeration value="KT"/>
										<xs:enumeration value=""/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="Country" type="ISOCountryCode">
								<xs:annotation>
									<xs:documentation xml:lang="lt">Šalies kodas iš dviejų raidžių pagal ISO 3166-1 alpha 2 arba ISO 3166-1 alpha 3 standartą, pavyzdžiui, NL arba NLD – Nyderlandai.</xs:documentation>
									<xs:documentation xml:lang="en">A 2-letter country code according to ISO 3166-1 alpha 2 or ISO 3166-1 alpha 3 standard, e.g. NL or NLD – the Netherlands.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
			<xs:element name="AccountID" type="SAFmiddle2textType">
				<xs:annotation>
					<xs:documentation xml:lang="lt">Subjekto apskaitos sistemoje naudojamos buhalterinės pirkėjo skolos sąskaitos kodas. Galima įtraukti subsąskaitų numerius. Gali apimti daug skirtingų lygių tam, kad būtų galima kuo geriau identifikuoti sąskaitą. Gali apimti išlaidų centrus, tokius kaip: įmonė, padalinys, regionas, grupė ir filialas / departamentas.</xs:documentation>
					<xs:documentation xml:lang="en">A code of the financial account of purchaser‘s debt used in the accounting system of the entity. Numbers of sub-accounts can be included. Can involve many different levels in order to identify the account as better as possible. Can cover cost centres such as: company, unit, region, group and branch / department.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="InvoiceDate" type="SAFdate" nillable="true">
				<xs:annotation>
					<xs:documentation xml:lang="lt">Sąskaitos faktūros data, PVM sąskaitos faktūros data.</xs:documentation>
					<xs:documentation xml:lang="en">Invoice date, VAT invoice date.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="InvoiceType">
				<xs:annotation>
					<xs:documentation xml:lang="lt">Pardavimo dokumento tipas. Pasirinkimai: „S“ (Sąskaita faktūra), „SF“ (PVM sąskaita faktūra), „D“ (Debetinė sąskaita), „DS“ (Debetinė PVM sąskaita faktūra), „K“ (Kreditinė sąskaita),  „KS“ (Kreditinė PVM sąskaita faktūra), „AN“ (Anuliuota), „VS“ (Viena (advokatų / notarų) PVM sąskaita faktūra), „VD“ (Viena (advokatų / notarų) PVM sąskaita faktūra debetinė),  „VK“ – (Viena (advokatų / notarų) PVM sąskaita faktūra kreditinė), „KT“ (Kita).</xs:documentation>
					<xs:documentation xml:lang="en">Type of sales document. Options: “S” (Invoice), “SF” (VAT invoice), “D” (Debit note), “DS” (VAT debit note), “K” (Credit note), “KS” (VAT credit note), “AN” (Annulled), “VS” (One (lawyers / notaries public) VAT invoice), “VD” (One (lawyers / notaries public) VAT debit note), “VK” (One (lawyers / notaries public) VAT credit note), “KT” (Other).</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="SAFcodeType">
						<xs:enumeration value="S"/>
						<xs:enumeration value="SF"/>
						<xs:enumeration value="D"/>
						<xs:enumeration value="DS"/>
						<xs:enumeration value="K"/>
						<xs:enumeration value="KS"/>
						<xs:enumeration value="AN"/>
						<xs:enumeration value="VS"/>
						<xs:enumeration value="VD"/>
						<xs:enumeration value="VK"/>
						<xs:enumeration value="KT"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ShipTo" type="ShippingPointStructure" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="lt">Pristatymo duomenys.</xs:documentation>
					<xs:documentation xml:lang="en">Delivery data.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ShipFrom" type="ShippingPointStructure" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="lt">Išsiuntimo duomenys.</xs:documentation>
					<xs:documentation xml:lang="en">Dispatch data.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SelfBillingIndicator" type="SAFcodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="lt">Žymima - „V“, kai PVM sąskaitą faktūrą / sąskaitą išsirašo pats prekių gavėjas.</xs:documentation>
					<xs:documentation xml:lang="en">A letter “V” is used for marking when a VAT invoice / account is issued by the consignee itself.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GLPostingDate" type="SAFdate" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="lt">Data, kada didžiojoje knygoje fiksuojamas ūkinės operacijos ar ūkinio įvykio įrašas.</xs:documentation>
					<xs:documentation xml:lang="en">Date when entry on economic transaction or economic event is made in the General ledger.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SystemID" type="SAFmiddle1textType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="lt">Sistemos automatiškai sukurtas unikalus kodas / numeris konkrečiam dokumentui, susijusiam su ūkiniu įvykiu.</xs:documentation>
					<xs:documentation xml:lang="en">Unique code / number automatically created by the system for a specific document relating to economic event.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TransactionID" type="SAFmiddle2textType">
				<xs:annotation>
					<xs:documentation xml:lang="lt">Unikalus numeris, kuris priskirtas ūkinei operacijai / įvykiui ir kuris gali būti grindžiamas viena ar keliomis serijomis.</xs:documentation>
					<xs:documentation xml:lang="en">A unique number assigned to economic transaction / event and which can be based on one or more series.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Line" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="lt">Sąskaitos eilutės / įrašo duomenys.</xs:documentation>
					<xs:documentation xml:lang="en">Data of account line / entry.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="LineNumber" type="SAFshorttextType">
							<xs:annotation>
								<xs:documentation xml:lang="lt">Sąskaitos eilutės / įrašo numeris.</xs:documentation>
								<xs:documentation xml:lang="en">Number of account line / entry.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="AccountID" type="SAFmiddle2textType">
							<xs:annotation>
								<xs:documentation xml:lang="lt">Subjekto apskaitos sistemoje naudojamos buhalterinės sąnaudų / atsargų / turto sąskaitos kodas. Galima įtraukti subsąskaitų numerius. Gali apimti daug skirtingų lygių tam, kad būtų galima kuo geriau identifikuoti sąskaitą. Gali apimti išlaidų centrus, tokius kaip: įmonė, padalinys, regionas, grupė ir filialas, departamentas.</xs:documentation>
								<xs:documentation xml:lang="en">A code of the financial account of income used in the accounting system of the entity. Numbers of sub-accounts can be included. Can involve many different levels in order to identify the account as better as possible. Can cover cost centres such as: company, unit, region, group and branch / department.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Analysis" type="AnalysisStructure" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation xml:lang="lt">Didžiosios knygos analizės kodai.</xs:documentation>
								<xs:documentation xml:lang="en">Analysis codes of the General ledger.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="OrderReferences" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation xml:lang="lt">Užsakymo duomenys.</xs:documentation>
								<xs:documentation xml:lang="en">Order data.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="OriginatingON" type="SAFmiddle2textType" minOccurs="0">
										<xs:annotation>
											<xs:documentation xml:lang="lt">Unikalus užsakymo numeris.</xs:documentation>
											<xs:documentation xml:lang="en">Unique order number.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="OrderDate" type="SAFdate" minOccurs="0">
										<xs:annotation>
											<xs:documentation xml:lang="lt">Užsakymo data.</xs:documentation>
											<xs:documentation xml:lang="en">Order date.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="ShipTo" type="ShippingPointStructure" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation xml:lang="lt">Pristatymo duomenys.</xs:documentation>
								<xs:documentation xml:lang="en">Delivery data.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ShipFrom" type="ShippingPointStructure" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation xml:lang="lt">Išsiuntimo duomenys.</xs:documentation>
								<xs:documentation xml:lang="en">Dispatch data.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="GoodsServicesID" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="lt">Požymis, rodantis, ar tai prekė, ar paslauga. Elemento reikšmės: „PR“ (Prekė), „PS“ (Paslauga), „IT“ (Ilgalaikis turtas), „KT“ (Kita).</xs:documentation>
								<xs:documentation xml:lang="en">An attribute showing whether this is a product or a service. Item values: “PR” (Product), “PS” (Service), “IT” (Non-current assets), “KT” (Other).</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="SAFcodeType">
									<xs:enumeration value="PS"/>
									<xs:enumeration value="PR"/>
									<xs:enumeration value="IT"/>
									<xs:enumeration value="KT"/>
									<xs:enumeration value=""/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="ProductCode" type="SAFmiddle2textType" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="lt">Prekės / paslaugos kodas.</xs:documentation>
								<xs:documentation xml:lang="en">Product/service code.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Delivery" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation xml:lang="lt">Informacija apie prekių ar paslaugų pristatymo (atlikimo) datą arba trukmę.</xs:documentation>
								<xs:documentation xml:lang="en">Information on date or duration of product delivery and service performance.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="MovementReference" type="SAFmiddle1textType">
										<xs:annotation>
											<xs:documentation xml:lang="lt">Unikali judėjimo (gabenimo) nuoroda (vardas, santrumpa, numeris), identifikuojanti atsargų judėjimo operaciją.</xs:documentation>
											<xs:documentation xml:lang="en">Unique movement (transportation) reference (forename, abbreviation, number) identifying a stock movement operation.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:choice>
										<xs:element name="DeliveryDate" type="SAFdate" nillable="true">
											<xs:annotation>
												<xs:documentation xml:lang="lt">Krovinio / siuntos pristatymo / išsiuntimo data. Atsižvelgiant į dokumentų rūšį ir krovinio siuntos pristatymo sąlygas, pasirenkama viena iš datų (pvz., esant pardavimo operacijai, pasirenkama sąskaitos data, išskyrus atvejus, kai pagal pristatymo sąlygas krovinio siuntos nuosavybė pereina pirkėjui jų pristatymo momentu).</xs:documentation>
												<xs:documentation xml:lang="en">Date of delivery/dispatch of consignment/shipment. One of the dates should be selected taking into account the document type and the terms of delivery of consignment/shipment (e.g. the invoice date should be selected in case of sales transaction, except in cases when as per terms of delivery the ownership to consignment/shipment passes to the purchaser at the moment of delivery thereof).</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="DeliveryPeriod">
											<xs:annotation>
												<xs:documentation xml:lang="lt">Pristatymų / atlikimų terminai.</xs:documentation>
												<xs:documentation xml:lang="en">Terms of deliveries / performances.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="FromDate" type="SAFdate" nillable="true">
														<xs:annotation>
															<xs:documentation xml:lang="lt">Pristatymų / atlikimų pradžios data.</xs:documentation>
															<xs:documentation xml:lang="en">Start date of deliveries / performances.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="ToDate" type="SAFdate" nillable="true">
														<xs:annotation>
															<xs:documentation xml:lang="lt">Pristatymų / atlikimų pabaigos data.</xs:documentation>
															<xs:documentation xml:lang="en">End date of deliveries / performances.</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:choice>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="Quantity" type="SAFquantityType" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="lt">Tiekiamų prekių ar teikiamų paslaugų kiekis.</xs:documentation>
								<xs:documentation xml:lang="en">Quantity of products supplied or services provided.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="InvoiceUOM" type="SAFcodeType" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="lt">Kiekio matavimo vienetas, pavyzdžiui, 12 vnt. pakuotė.</xs:documentation>
								<xs:documentation xml:lang="en">Unit of measurement of quantity, e.g. a package of 12 pcs.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="UOMToUOMBaseConversionFactor" type="xs:decimal" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="lt">Matavimo vienetų konversijos koeficientas, keičiant iš standartinio į bazinį, pvz., jei standartinis matavimo vienetas yra ml, o bazinis – L, tuomet matavimo vienetų konversijos koeficientas yra 0,001.</xs:documentation>
								<xs:documentation xml:lang="en">Conversion factor for units of measurement when changed from standard to basic, e.g. if a standard unit of measurement is ml and a basis one is L, then a conversion factor for units of measurement is 0,001.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="UnitPrice" type="xs:decimal" nillable="true">
							<xs:annotation>
								<xs:documentation xml:lang="lt">Vieneto kaina vienetui / vienetų grupės kiekiui antraštėje nurodyta valiuta.</xs:documentation>
								<xs:documentation xml:lang="en">Unit price for a unit / quantity of a group of units in a currency specified in the heading.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="TaxPointDate" type="SAFdate" nillable="true">
							<xs:annotation>
								<xs:documentation xml:lang="lt">Nurodyta apmokestinimo data, jei data nenurodyta, tuomet sąskaitos data.</xs:documentation>
								<xs:documentation xml:lang="en">Taxation date specified; invoice date if no date is specified.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Description" type="SAFlongtextType">
							<xs:annotation>
								<xs:documentation xml:lang="lt">Sąskaitos eilutės aprašymas.</xs:documentation>
								<xs:documentation xml:lang="en">Description of account line.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="InvoiceLineAmount" type="AmountStructure">
							<xs:annotation>
								<xs:documentation xml:lang="lt">Operacijos suma, neįtraukiant mokesčių ir gabenimo rinkliavų / išlaidų.</xs:documentation>
								<xs:documentation xml:lang="en">Transaction amount, not including taxes and transportation levies / costs.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="DebitCreditIndicator">
							<xs:annotation>
								<xs:documentation xml:lang="lt">Nuoroda, identifikuojanti eilutės sumos požymį (D - debetą / K - kreditą). Įrašas privalo atitikti įrašą, randamą didžiojoje knygoje. Pavyzdžiui, grąžinimas gali būti nurodytas neigiama suma (kontrarinėje sąskaitoje).</xs:documentation>
								<xs:documentation xml:lang="en">Reference identifying an attribute of the line‘s amount (D – debit / K – credit). The entry must correspond to the entry found in the General ledger. For example, refund can be specified in a negative amount (in a contrary account).</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="SAFcodeType">
									<xs:enumeration value="D"/>
									<xs:enumeration value="K"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="ShippingCostsAmount" type="AmountStructure" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="lt">Gabenimo išlaidų suma.</xs:documentation>
								<xs:documentation xml:lang="en">Amount of transport costs.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="TaxInformation" type="TaxInformationStructure" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation xml:lang="lt">Informacija apie mokesčius.</xs:documentation>
								<xs:documentation xml:lang="en">Information on taxes.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="References" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="lt">Kreditinio / debetinio dokumento nuoroda.</xs:documentation>
					<xs:documentation xml:lang="en">Reference of credit / debit document.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="CreditNote" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation xml:lang="lt">Kreditinis / debetinis dokumentas.</xs:documentation>
								<xs:documentation xml:lang="en">Credit / debit document.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Reference" type="SAFmiddle1textType" minOccurs="0">
										<xs:annotation>
											<xs:documentation xml:lang="lt">Kreditinio / debetinio dokumento nuoroda į tikslinamą sąskaitą.</xs:documentation>
											<xs:documentation xml:lang="en">Reference of credit / debit document to a revised note.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="Reason" type="SAFlongtextType" minOccurs="0">
										<xs:annotation>
											<xs:documentation xml:lang="lt">Kreditinio / debetinio dokumento išrašymo priežastis arba pagrindas.</xs:documentation>
											<xs:documentation xml:lang="en">Reason of or basis for issuing a credit / debit document.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Settlement" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="lt">Informacija apie pardavimo sąskaitai taikytą kaupiamąją nuolaidą.</xs:documentation>
					<xs:documentation xml:lang="en">Information on a cumulative rebate applied for the purchase account.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="SettlementDiscount" type="SAFmiddle1textType" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="lt">Kaupiamosios nuolaidos (diskontų ir kt.) aprašymas, pvz., 10 proc., jei sąskaita bus apmokėta per 20 d.</xs:documentation>
								<xs:documentation xml:lang="en">Description of cumulative rebate (discounts, etc.), e.g. 10% if the invoice will be paid within 20 days.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="SettlementAmount" type="AmountStructure">
							<xs:annotation>
								<xs:documentation xml:lang="lt">Informacija apie kaupiamosios nuolaidos sumą.</xs:documentation>
								<xs:documentation xml:lang="en">Information on an amount of cumulative rebate.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="SettlementDueDate" type="SAFmiddle1textType" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="lt">Atsiskaitymo terminas, pvz., 20 d.</xs:documentation>
								<xs:documentation xml:lang="en">Time frame for settlement, e.g. 20 days.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="SettlementDate" type="SAFdate" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="lt">Atsiskaitymo data, kuriai suėjus bus pritaikyta kaupiamoji nuolaida.</xs:documentation>
								<xs:documentation xml:lang="en">Settlement date after which a cumulative rebate will be applied.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="PaymentMechanism" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="lt">Mokėjimo būdas. Pasirinkimai: „GR“ (grynaisiais), „NG“ (negrynaisiais), „U“ (užskaita).</xs:documentation>
								<xs:documentation xml:lang="en">Payment method. Options: “GR” (cash), “NG” (non-cash), “U” (netting).</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="SAFcodeType">
									<xs:enumeration value="GR"/>
									<xs:enumeration value="NG"/>
									<xs:enumeration value="U"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="DebitCreditIndicator">
							<xs:annotation>
								<xs:documentation xml:lang="lt">Debeto arba kredito požymis (D - debetas, K- kreditas).</xs:documentation>
								<xs:documentation xml:lang="en">Debit or credit attribute (D – debit / K – credit).</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="SAFcodeType">
									<xs:enumeration value="D"/>
									<xs:enumeration value="K"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="DocumentTotals" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="lt">Dokumento suminiai rezultatai.</xs:documentation>
					<xs:documentation xml:lang="en">Total results of document.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="TaxInformationTotals" type="TaxInformationStructure" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation xml:lang="lt">Apskaičiuotų mokesčių suma, apibendrinama pagal mokesčių tipus / mokesčių kodus.</xs:documentation>
								<xs:documentation xml:lang="en">Amount of estimated taxes summed up according to tax types / tax codes.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ShippingCostsAmountTotal" type="SAFmonetaryType" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="lt">Bendra gabenimo išlaidų suma.</xs:documentation>
								<xs:documentation xml:lang="en">Total amount of transport costs.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="NetTotal" type="SAFmonetaryType" nillable="true">
							<xs:annotation>
								<xs:documentation xml:lang="lt">Bendra pardavimo vertė, neįtraukiant mokesčių ir gabenimo išlaidų.</xs:documentation>
								<xs:documentation xml:lang="en">Total value of purchases not including taxes and transport costs.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="GrossTotal" type="SAFmonetaryType">
							<xs:annotation>
								<xs:documentation xml:lang="lt">Bendra suma, įskaitant mokesčius ir gabenimo išlaidas.</xs:documentation>
								<xs:documentation xml:lang="en">Total amount including taxes and transport costs.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="GLTransactionID" type="SAFmiddle2textType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="lt">Elementas, skirtas pirminiam dokumentui su Didžiosios knygos elementu 3.4.4.1 „Ūkinės operacijos ar ūkinio įvykio Nr.“ susieti.</xs:documentation>
					<xs:documentation xml:lang="en">Element intended for linking of the initial document with the element of the General ledger 3.4.4.1 “Number of economic transaction or economic event“.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ShippingPointStructure">
		<xs:annotation>
			<xs:documentation xml:lang="lt">Pristatymų informacijos struktūra.</xs:documentation>
			<xs:documentation xml:lang="en">Delivery Information structure.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DeliveryID" type="SAFmiddle1textType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="lt">Krovinio / siuntos identifikavimo numeris. Pateikiamas važtaraščio Nr. Jei prekės vežamos su keliais krovinio važtaraščiais, pateikiami visų važtaraščių Nr.</xs:documentation>
					<xs:documentation xml:lang="en">Consignment / shipment identification number. Number of waybill should be provided. Numbers of all waybills should be shown if the products shipped are accompanied by several waybills
</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DeliveryDate" type="SAFdate" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="lt">Krovinio / siuntos pristatymo / išsiuntimo data. Atsižvelgiant į dokumentų rūšį ir krovinio siuntos pristatymo sąlygas, pasirenkama viena iš datų (pvz., esant pardavimo operacijai, pasirenkama sąskaitos data, išskyrus atvejus, kai pagal pristatymo sąlygas krovinio siuntos nuosavybė pereina pirkėjui jų pristatymo momentu).</xs:documentation>
					<xs:documentation xml:lang="en">Date of delivery/dispatch of consignment/shipment. One of the dates should be selected taking into account the document type and the terms of delivery of consignment/shipment (e.g. the invoice date should be selected in case of sales transaction, except in cases when as per terms of delivery the ownership to consignment/shipment passes to the purchaser at the moment of delivery thereof).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WarehouseID" type="SAFmiddle1textType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="lt">Sandėlis, kuriame laikomos prekės, taip pat įmanoma identifikuoti prekių gamybos arba atsargų (prekių) judėjimo metu.</xs:documentation>
					<xs:documentation xml:lang="en">Warehouse in which the goods are stored. It can be also identified during manufacture of products or movement of stock (goods).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Address" type="AddressStructure" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="lt">Adresas.</xs:documentation>
					<xs:documentation xml:lang="en">Address.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AddressStructure">
		<xs:annotation>
			<xs:documentation xml:lang="lt">Adreso struktūra.</xs:documentation>
			<xs:documentation xml:lang="en">Address Structure.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="StreetName" type="SAFmiddle2textType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="lt">Gatvės pavadinimas.</xs:documentation>
					<xs:documentation xml:lang="en">Street name.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Number" type="SAFshorttextType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="lt">Pastato, buto numeris.</xs:documentation>
					<xs:documentation xml:lang="en">Building, apartment number.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="City" type="SAFmiddle1textType">
				<xs:annotation>
					<xs:documentation xml:lang="lt">Miestas.</xs:documentation>
					<xs:documentation xml:lang="en">City.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PostalCode" type="SAFshorttextType">
				<xs:annotation>
					<xs:documentation xml:lang="lt">Pašto kodas.</xs:documentation>
					<xs:documentation xml:lang="en">Postal code.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Country" type="ISOCountryCode">
				<xs:annotation>
					<xs:documentation xml:lang="lt">Šalies kodas iš dviejų raidžių pagal ISO 3166-1 alpha 2 arba ISO 3166-1 alpha 3 standartą, pavyzdžiui, NL arba NLD – Nyderlandai. Teikiant tikslinės teritorijos, neturinčios kodo pagal ISO 3166-1, informaciją, elemente „Region“ pateikiamas kodas pagal ISO 3166-2 standartą, o šiame elemente nurodomas valstybės, kuriai priklauso regionas, kodas pagal ISO 3166-1 (pvz. PT).</xs:documentation>
					<xs:documentation xml:lang="en">A 2-letter country code according to ISO 3166-1 alpha 2 or ISO 3166-1 alpha 3 standard, e.g. NL or NLD – the Netherlands. Providing the information of target territories that does not have the country code according to ISO 3166-1, fill the “Region” element according to ISO 3166-2 standard, and to this “Country” element provide the information of the country code to which the region belongs according to ISO 3166-1, e.g. PT.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AddressType">
				<xs:annotation>
					<xs:documentation xml:lang="lt">Eilutė, skirta keliems adresams atskirti, nurodant adreso tipą. Iš galimo reikšmių sąrašo šiame elemente galimi pasirinkimai: „BA“ (Buveinės adresas), „KA“ (Korespondencijos adresas), „SIA“ (Sąskaitos išrašymo adresas), „RA“ (Registracijos adresas), „PIA“ (Prekės išsiuntimo adresas), „SGA“ (Sąskaitos gavimo adresas), „PPA“ (Prekės pristatymo adresas), „KT“ (Kitas adresas).</xs:documentation>
					<xs:documentation xml:lang="en">A line for separating several addresses by specifying an address type. Options available in this item from the possible list of values: “BA“ (Address of the Registered Office), “KA“ (Mailing address), “RA” (Registration address), “PIA” (Shipping address), “KT” (Other address).</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="BA"/>
						<xs:enumeration value="KA"/>
						<xs:enumeration value="SIA"/>
						<xs:enumeration value="RA"/>
						<xs:enumeration value="PIA"/>
						<xs:enumeration value="SGA"/>
						<xs:enumeration value="PPA"/>
						<xs:enumeration value="KT"/>
						<xs:enumeration value=""/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Region" type="SAFmiddle1textType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="lt">Savivaldybė. Teikiant informaciją apie tikslinę teritoriją, neturinčią kodo pagal ISO 3166-1 (pvz. Azorų salos),  nurodomas regiono kodas pagal ISO 3166-2 standartą (pvz., „PT-20“).</xs:documentation>
					<xs:documentation xml:lang="en">Municipality. Providing the information of target territories that does not have the country code according to ISO 3166-1 (e.g. the Azores), fill the region code according to ISO 3166-2 standard (e.g. “PT-20”).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FullAddress" type="SAFlongtextType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="lt">Adresas, rašomas laisvu tekstu (gatvė, namo numeris, miestas ir kt.), užpildomas tuo atveju, kai nėra galimybės pateikti struktūrizuoto.</xs:documentation>
					<xs:documentation xml:lang="en">A free text address (street, house number, city, etc.). Should be entered when there is no possibility to submit a structured address.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AmountStructure">
		<xs:annotation>
			<xs:documentation xml:lang="lt">Sumų struktūra.</xs:documentation>
			<xs:documentation xml:lang="en">Amount structure.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Amount" type="SAFmonetaryType" nillable="true">
				<xs:annotation>
					<xs:documentation xml:lang="lt">Suma, įvertinta antraštėje nurodyta valiuta.</xs:documentation>
					<xs:documentation xml:lang="en">Amount measured in a currency specified in the heading.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:sequence minOccurs="0">
				<xs:element name="CurrencyCode" type="ISOCurrencyCode">
					<xs:annotation>
						<xs:documentation xml:lang="lt">Trijų raidžių valiutos kodas pagal ISO 4217 standartą, pavyzdžiui, USD – JAV doleriams.</xs:documentation>
						<xs:documentation xml:lang="en">A 3-letter currency code according to ISO 4217 standard, e.g. USD – for US dollars.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CurrencyAmount" type="SAFmonetaryType" nillable="true">
					<xs:annotation>
						<xs:documentation xml:lang="lt">Suma atitinkama valiuta.</xs:documentation>
						<xs:documentation xml:lang="en">Amount in a relevant currency.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AnalysisStructure">
		<xs:annotation>
			<xs:documentation xml:lang="lt">Analizės struktūra.</xs:documentation>
			<xs:documentation xml:lang="en">Analysis structure.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AnalysisType" type="SAFcodeType">
				<xs:annotation>
					<xs:documentation xml:lang="lt">Subjekto apskaitos sistemoje naudojamas vidinio analizės tipo identifikatorius, pvz., KC (kaštų centrui), DEP (departamentui) ir t. t.</xs:documentation>
					<xs:documentation xml:lang="en">Identifier of internal analysis type used in the accounting system of the entity, e.g. CC (for a centre of costs, DEP (for department), etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AnalysisID" type="SAFlongtextType">
				<xs:annotation>
					<xs:documentation xml:lang="lt">Subjekto apskaitos sistemoje naudojamas vidinis analizės numeris, pvz., 1200-HDOF-TR (pagr. ofiso išlaikymo sąnaudos), P-4800-123 (123 projektui 4800 filiale).</xs:documentation>
					<xs:documentation xml:lang="en">Internal analysis number used in the accounting system of the entity, e.g. 1200-HDOF-TR (head office maintenance costs), P-4800-123 (for the project 123 in the 4800 branch).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AnalysisAmount" type="AmountStructure" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="lt">Suma konkrečiam analizės tipui.</xs:documentation>
					<xs:documentation xml:lang="en">Amount (amount of costs) for a specific analysis type.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
