    <Key>FetchGoodsTurnoverInfoList</Key>
    <Statement>SELECT g.ID, g.Name, g.MeasureUnit, l.AccountPurchases, l.AccountSalesNetCosts, l.AccountDiscounts, l.AccountValueReduction, r.Name, CROUND(g.PricePurchase, 6), CROUND(g.DefaultVatRatePurchase, 2), CROUND(g.PriceSale, 2), CROUND(g.DefaultVatRateSales, 2), g.DefaultAccountingMethod, IFNULL((SELECT v.MethodCode FROM goodsvaluationmethods v WHERE v.GoodsID=g.ID AND v.ChangeDate&lt;=$DT ORDER BY v.ChangeDate DESC LIMIT 1), g.DefaultValuationMethod) AS CurrentValuationMethod, g.TradeItemType, g.GoodsCode, g.GoodsBarCode, w.Name, w.WarehouseAccount, g.IsObsolete, CROUND(SUM(CASE WHEN o.OperationDate&lt;$DF THEN o.Amount ELSE 0 END), 6) AS AmountPeriodStart, CROUND(SUM(CASE WHEN o.OperationDate&lt;$DF THEN o.AmountInWarehouse ELSE 0 END), 6) AS AmountInWarehousePeriodStart, SUM(CASE WHEN o.OperationDate&gt;=$DF THEN o.Amount ELSE 0 END) AS AmountChange, SUM(CASE WHEN o.OperationDate&gt;=$DF THEN o.AmountInWarehouse ELSE 0 END) AS AmountInWarehouseChange, SUM(CASE WHEN o.OperationDate&gt;=$DF THEN o.AmountInPurchases ELSE 0 END) AS AmountPurchasesChange, CROUND(SUM(CASE WHEN o.OperationType=1 AND o.OperationDate&gt;=$DF THEN o.Amount ELSE 0 END), 6) AS AmountAcquisitions, CROUND(SUM(CASE WHEN o.OperationType=2 AND o.OperationDate&gt;=$DF THEN o.Amount ELSE 0 END), 6) AS AmountTransfered, CROUND(SUM(CASE WHEN o.OperationType=3 AND o.OperationDate&gt;=$DF THEN o.Amount ELSE 0 END), 6) AS AmountDiscarded, CROUND(SUM(CASE WHEN o.OperationType=1 AND o.OperationDate&gt;=$DF THEN o.AmountInWarehouse ELSE 0 END), 6) AS AmountAcquisitionsInWarehouse, CROUND(SUM(CASE WHEN o.OperationType=2 AND o.OperationDate&gt;=$DF THEN o.AmountInWarehouse ELSE 0 END), 6) AS AmountTransferedInWarehouse, CROUND(SUM(CASE WHEN o.OperationType=3 AND o.OperationDate&gt;=$DF THEN o.AmountInWarehouse ELSE 0 END), 6) AS AmountDiscardedInWarehouse, CROUND(SUM(CASE WHEN o.OperationType=4 AND o.OperationDate&gt;=$DF THEN o.Amount ELSE 0 END), 6) AS AmountChangeInventorization, CROUND(SUM(o.AmountInWarehouse), 6) AS AmountInWarehousePeriodEnd, CROUND(SUM(CASE WHEN o.OperationDate&lt;$DF THEN o.AmountInPurchases ELSE 0 END), 6) AS AmountPurchasesPeriodStart, CROUND(SUM(o.AmountInPurchases), 6) AS AmountPurchasesPeriodEnd, CROUND(SUM(CASE WHEN o.OperationType=6 AND o.OperationDate&gt;=$DF THEN o.TotalValue ELSE 0 END), 2) AS TotalAdditionalCosts, CROUND(SUM(CASE WHEN o.OperationType=5 AND o.OperationDate&gt;=$DF THEN o.TotalValue ELSE 0 END), 2) AS TotalDiscounts, CROUND(SUM(CASE WHEN o.OperationType=6 AND o.OperationDate&gt;=$DF THEN o.AccountSalesNetCosts ELSE 0 END), 2) AS TotalAdditionalCostsForDiscardedGoods, CROUND(SUM(CASE WHEN o.OperationType=5 AND o.OperationDate&gt;=$DF THEN o.AccountSalesNetCosts ELSE 0 END), 2) AS TotalDiscountsForDiscardedGoods, CROUND(SUM(CASE WHEN o.OperationDate&lt;$DF THEN o.AccountPurchases ELSE 0 END), 2) AS AccountPurchasesPeriodStart, CROUND(SUM(CASE WHEN o.OperationDate&gt;=$DF AND o.AccountPurchases&gt;0 THEN o.AccountPurchases ELSE 0 END), 2) AS AccountPurchasesDebit, CROUND(SUM(CASE WHEN o.OperationDate&gt;=$DF AND o.AccountPurchases&lt;0 THEN o.AccountPurchases ELSE 0 END), 2) AS AccountPurchasesCredit, CROUND(SUM(CASE WHEN o.OperationDate&lt;$DF THEN o.AccountGeneral ELSE 0 END), 2) AS AccountWarehousePeriodStart, CROUND(SUM(CASE WHEN o.OperationDate&gt;=$DF AND o.AccountGeneral&gt;0 THEN o.AccountGeneral ELSE 0 END), 2) AS AccountWarehouseDebit, CROUND(SUM(CASE WHEN o.OperationDate&gt;=$DF AND o.AccountGeneral&lt;0 THEN o.AccountGeneral ELSE 0 END), 2) AS AccountWarehouseCredit, CROUND(SUM(CASE WHEN o.OperationDate&lt;$DF THEN o.AccountSalesNetCosts ELSE 0 END), 2) AS AccountSalesNetCostsPeriodStart, CROUND(SUM(CASE WHEN o.OperationDate&gt;=$DF AND o.AccountSalesNetCosts&gt;0 THEN o.AccountSalesNetCosts ELSE 0 END), 2) AS AccountSalesNetCostsDebit, CROUND(SUM(CASE WHEN o.OperationDate&gt;=$DF AND o.AccountSalesNetCosts&lt;0 THEN o.AccountSalesNetCosts ELSE 0 END), 2) AS AccountSalesNetCostsCredit, CROUND(SUM(CASE WHEN o.OperationDate&lt;$DF THEN o.AccountDiscounts ELSE 0 END), 2) AS AccountDiscountsPeriodStart, CROUND(SUM(CASE WHEN o.OperationDate&gt;=$DF AND o.AccountDiscounts&gt;0 THEN o.AccountDiscounts ELSE 0 END), 2) AS AccountDiscountsDebit, CROUND(SUM(CASE WHEN o.OperationDate&gt;=$DF AND o.AccountDiscounts&lt;0 THEN o.AccountDiscounts ELSE 0 END), 2) AS AccountDiscountsCredit, CROUND(SUM(CASE WHEN o.OperationDate&lt;$DF THEN o.AccountPriceCut ELSE 0 END), 2) AS AccountValueReductionPeriodStart, CROUND(SUM(CASE WHEN o.OperationDate&gt;=$DF AND o.AccountPriceCut&gt;0 THEN o.AccountPriceCut ELSE 0 END), 2) AS AccountValueReductionDebit, CROUND(SUM(CASE WHEN o.OperationDate&gt;=$DF AND o.AccountPriceCut&lt;0 THEN o.AccountPriceCut ELSE 0 END), 2) AS AccountValueReductionCredit FROM goods g LEFT JOIN prekes_gr r ON r.ID=g.GroupID LEFT JOIN warehouses w ON w.ID=g.DefaultWarehouseID LEFT JOIN goodsoperations o ON o.GoodsID=g.ID AND o.OperationDate&lt;=$DT AND ($WD&lt;1 OR o.WarehouseID=$WD) LEFT JOIN goodscomplexoperations c ON c.ID=o.ComplexOperationID LEFT JOIN (SELECT go.ID AS ID, IFNULL(MAX(CASE WHEN ro.AccountType=9 THEN ro.AccountValue ELSE null END), go.AccountSalesNetCosts) AS AccountSalesNetCosts, IFNULL(MAX(CASE WHEN ro.AccountType=10 THEN ro.AccountValue ELSE null END), go.AccountPurchases) AS AccountPurchases, IFNULL(MAX(CASE WHEN ro.AccountType=11 THEN ro.AccountValue ELSE null END), go.AccountDiscounts) AS AccountDiscounts, IFNULL(MAX(CASE WHEN ro.AccountType=12 THEN ro.AccountValue ELSE null END), go.AccountValueReduction) AS AccountValueReduction FROM goods go LEFT JOIN (SELECT po.GoodsID AS GoodsID, po.AccountType AS AccountType, po.AccountValue AS AccountValue FROM (SELECT ac.GoodsID AS GoodsID, ac.AccountType AS AccountType, MAX(ac.ChangeDate) AS MaxDate FROM goodsaccounts ac WHERE ac.ChangeDate&lt;$DT GROUP BY ac.GoodsID, ac.AccountType) AS vo LEFT JOIN goodsaccounts po ON po.GoodsID=vo.GoodsID AND po.AccountType=vo.AccountType AND po.ChangeDate=vo.MaxDate WHERE NOT po.GoodsID IS NULL) AS ro ON ro.GoodsID=go.ID GROUP BY go.ID) AS l ON l.ID=g.ID WHERE ($GD&lt;1 OR g.GroupID=$GD) AND ($WD&gt;0 OR c.OperationType IS NULL OR c.OperationType&lt;&gt;1) AND (UPPER(g.Name) LIKE UPPER($TX) OR UPPER(g.GoodsCode) LIKE UPPER($TX)) GROUP BY g.ID;</Statement>
