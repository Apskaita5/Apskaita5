    <Key>FetchDeclarationFR0600_2</Key>
    <Statement>SELECT l.FieldCode, SUM(l.FieldValue) FROM (SELECT e.FieldCode AS FieldCode, CROUND((CASE WHEN e.IsVatField &gt; 0 THEN d.SumVatLTL ELSE d.SumLTL END) * (CASE WHEN e.MinusValue &gt; 0 THEN -1 ELSE 1 END) * e.InclusionPercentage / 100, 2) AS FieldValue FROM bz b LEFT JOIN sfd d ON d.SF_ID=b.Op_ID LEFT JOIN VatDeclarationEntrys e ON e.ParentID=d.DeclarationSchemaID WHERE d.DeclarationSchemaID &gt; 0 AND b.Op_Data &gt;= $DF AND b.Op_Data &lt;= $DT UNION SELECT e.FieldCode AS FieldCode, CROUND((CASE WHEN e.IsVatField &gt; 0 THEN d.SumVatLTL ELSE d.SumLTL END) * (CASE WHEN e.MinusValue &gt; 0 THEN -1 ELSE 1 END) * e.InclusionPercentage / 100, 2) AS FieldValue FROM bz b LEFT JOIN sfg d ON d.BZ_ID=b.Op_ID LEFT JOIN VatDeclarationEntrys e ON e.ParentID=d.DeclarationSchemaID WHERE d.DeclarationSchemaID &gt; 0 AND b.Op_Data &gt;= $DF AND b.Op_Data &lt;= $DT UNION SELECT e.FieldCode AS FieldCode, CROUND((CASE WHEN e.IsVatField &gt; 0 THEN i.IndirectVatSum ELSE 0 END) * (CASE WHEN e.MinusValue &gt; 0 THEN -1 ELSE 1 END) * e.InclusionPercentage / 100, 2) AS FieldValue FROM bz b LEFT JOIN InvoicesReceived i ON i.ID=b.Op_ID LEFT JOIN VatDeclarationEntrys e ON e.ParentID=i.DeclarationSchemaID WHERE i.DeclarationSchemaID &gt; 0 AND b.Op_Data &gt;= $DF AND b.Op_Data &lt;= $DT UNION SELECT e.FieldCode AS FieldCode, CROUND((CASE WHEN e.IsVatField &gt; 0 THEN d.SumVatLTL ELSE d.SumLTL END) * (CASE WHEN e.MinusValue &gt; 0 THEN -1 ELSE 1 END) * e.InclusionPercentage / 100, 2) AS FieldValue FROM bz b LEFT JOIN apyskaitos d ON d.BZ_ID=b.Op_ID LEFT JOIN VatDeclarationEntrys e ON e.ParentID=d.DeclarationSchemaID WHERE d.DeclarationSchemaID &gt; 0 AND NOT d.SG&gt;0 AND b.Op_Data &gt;= $DF AND b.Op_Data &lt;= $DT) AS l GROUP BY l.FieldCode;</Statement>
