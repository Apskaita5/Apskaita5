    <Statement>INSERT INTO invoicesmade(ID, InvoiceSerial, InvoiceNumber, CurrencyCode, CurrencyRate, LanguageCode, VatExemptions, VatExemptionsAltLng, CustomInfo, CustomInfoAltLng, CommentsInternal, AddDateToNumberOptionWasUsed, NumbersInInvoice, AccountPayer, InsertDate, UpdateDate) SELECT b.Op_ID, s.Serija, SUBSTRING(b.Op_Dok, LENGTH(s.Serija)+1), 'LTL', 1, 'LT', '', '', '', '', '', 0, 0, IFNULL(IF(f.ID IS NULL, d.Op_saskaita, 0), 0), UTC_TIMESTAMP(), UTC_TIMESTAMP() FROM bz b LEFT JOIN serijos s ON s.Serijos_dok='Sąskaita - faktūra' AND LOCATE(s.Serija, b.Op_Dok)=1 AND SUBSTRING(b.Op_Dok, LENGTH(s.Serija)+1,1) REGEXP '^[0-9]+$' LEFT JOIN bzdata d ON d.Op_tipas='Debetas' AND d.Op_ID_BZ=b.Op_ID LEFT JOIN sfd f ON f.SF_ID=b.Op_ID AND (f.S_Sas=d.Op_saskaita OR f.P_Sas=d.Op_saskaita) WHERE b.Op_dok_rusis='sf';</Statement>
