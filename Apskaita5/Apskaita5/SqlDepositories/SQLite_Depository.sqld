    <Statement>SELECT c.ID, c.ParentID, c.AcquisitionID, c.WarehouseID, c.ConsignmentAmount, c.UnitValue, c.TotalValue, c.AmountWithdrawn, c.TotalValueWithdrawn, w.Name, b.Op_data, b.Op_Dok, b.Op_dok_rusis, d.TotalValue, p.TotalValue, d.ID, p.ID FROM (SELECT c.ID AS ID, c.ParentID AS ParentID, c.AcquisitionID AS AcquisitionID, c.WarehouseID AS WarehouseID, CROUND(c.Amount, 6) AS ConsignmentAmount, c.UnitValue AS UnitValue, c.TotalValue AS TotalValue, CROUND(IFNULL(SUM(d.Amount), 0), 6) AS AmountWithdrawn, IFNULL(SUM(d.TotalValue), 0) AS TotalValueWithdrawn, o.JournalEntryID AS JournalEntryID FROM consignments c LEFT JOIN consignmentdiscards d ON d.ConsignmentID=c.ID AND d.ParentID&lt;&gt;$OD LEFT JOIN goodsoperations o ON o.ID=c.AcquisitionID WHERE c.ParentID&lt;&gt;$OD AND o.GoodsID=$GD AND o.WarehouseID=$WD GROUP BY c.ID HAVING ConsignmentAmount &gt; AmountWithdrawn ORDER BY o.OperationDate ASC) AS c LEFT JOIN bz b ON b.Op_ID=c.JournalEntryID LEFT JOIN warehouses w ON w.ID=c.WarehouseID LEFT JOIN consignmentdiscards d ON d.ParentID=$OD AND d.ConsignmentID=c.ID LEFT JOIN consignments p ON p.ParentID=$OD AND p.AcquisitionID=c.AcquisitionID;</Statement>
