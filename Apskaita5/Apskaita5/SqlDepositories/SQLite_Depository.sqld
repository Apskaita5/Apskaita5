  <MySQL_Statement>
    <Key>FetchVatDeclaration</Key>
    <Statement>SELECT b.Op_ID, b.Op_data, b.Op_Dok, b.Op_turinys, b.Op_dok_rusis, d.Preke, d.SumLTL-d.DiscountLTL, d.Tar, d.SumVatLTL-d.DiscountVatLTL, e.FieldCode, CROUND((CASE WHEN e.IsVatField &gt; 0 THEN d.SumVatLTL-d.DiscountVatLTL ELSE d.SumLTL-d.DiscountLTL END) * (CASE WHEN e.MinusValue &gt; 0 THEN -1 ELSE 1 END) * e.InclusionPercentage / 100, 2) AS FieldValue FROM bz b LEFT JOIN sfd d ON d.SF_ID=b.Op_ID LEFT JOIN VatDeclarationEntrys e ON e.ParentID=d.DeclarationSchemaID WHERE d.DeclarationSchemaID &gt; 0 AND b.Op_Data &gt;= $DF AND b.Op_Data &lt;= $DT UNION ALL SELECT b.Op_ID, b.Op_data, b.Op_Dok, b.Op_turinys, b.Op_dok_rusis, d.Preke, d.SumLTL, d.Tarif, d.SumVatLTL, e.FieldCode, CROUND((CASE WHEN e.IsVatField &gt; 0 THEN d.SumVatLTL ELSE d.SumLTL END) * (CASE WHEN e.MinusValue &gt; 0 THEN -1 ELSE 1 END) * e.InclusionPercentage / 100, 2) AS FieldValue FROM bz b LEFT JOIN sfg d ON d.BZ_ID=b.Op_ID LEFT JOIN VatDeclarationEntrys e ON e.ParentID=d.DeclarationSchemaID WHERE d.DeclarationSchemaID &gt; 0 AND b.Op_Data &gt;= $DF AND b.Op_Data &lt;= $DT UNION ALL SELECT b.Op_ID, b.Op_data, b.Op_Dok, b.Op_turinys, b.Op_dok_rusis, CASE WHEN b.Op_ID&gt;0 THEN $IV ELSE NULL END, 0, 0, i.IndirectVatSum, e.FieldCode, CROUND((CASE WHEN e.IsVatField &gt; 0 THEN i.IndirectVatSum ELSE 0 END) * (CASE WHEN e.MinusValue &gt; 0 THEN -1 ELSE 1 END) * e.InclusionPercentage / 100, 2) AS FieldValue FROM bz b LEFT JOIN InvoicesReceived i ON i.ID=b.Op_ID LEFT JOIN VatDeclarationEntrys e ON e.ParentID=i.DeclarationSchemaID WHERE i.DeclarationSchemaID &gt; 0 AND b.Op_Data &gt;= $DF AND b.Op_Data &lt;= $DT UNION ALL SELECT b.Op_ID, b.Op_data, b.Op_Dok, b.Op_turinys, b.Op_dok_rusis, d.Aprasas, d.SumLTL, d.PVMTarifas, d.SumVatLTL, e.FieldCode, CROUND((CASE WHEN e.IsVatField &gt; 0 THEN d.SumVatLTL ELSE d.SumLTL END) * (CASE WHEN e.MinusValue &gt; 0 THEN -1 ELSE 1 END) * e.InclusionPercentage / 100, 2) AS FieldValue FROM bz b LEFT JOIN apyskaitos d ON d.BZ_ID=b.Op_ID LEFT JOIN VatDeclarationEntrys e ON e.ParentID=d.DeclarationSchemaID WHERE d.DeclarationSchemaID &gt; 0 AND NOT d.SG&gt;0 AND b.Op_Data &gt;= $DF AND b.Op_Data &lt;= $DT;</Statement>
    <Objects>ActiveReports.VatDeclaration</Objects>
